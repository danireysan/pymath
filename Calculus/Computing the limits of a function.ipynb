   "source": [
    "x = sym.symbols('x') \n",
    "\n",
    "fx = x**3\n",
    "\n",
    "lim_pnt = 1.5\n",
    "lim = sym.limit(fx, x, lim_pnt)\n",
    "\n",
    "display(Math(\"\\\\lim_{x \\\\to %g} %s = %g\" %(lim_pnt, sym.latex(fx), lim)))"
